<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="root.DeleteAccountController">
   <children>
      <Label layoutX="70.0" layoutY="47.0" text="请输入要删除的账号：" textFill="#d71414">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="DEL_ACCOUNT" layoutX="65.0" layoutY="94.0" />
      <Button fx:id="DeleteButton" layoutX="77.0" layoutY="156.0" mnemonicParsing="false" onAction="#onChooseDeleteButton" text="删除" />
      <Button fx:id="CloseButton" layoutX="169.0" layoutY="156.0" mnemonicParsing="false" onAction="#onChooseCloseButton" text="关闭" />
   </children>
</AnchorPane>
